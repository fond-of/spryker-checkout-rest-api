{
  "name": "fond-of-spryker/checkout-rest-api",
  "type": "library",
  "description": "FondOf b2b checkout rest api module",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "fond-of-spryker/checkout-rest-api-extension": "dev-master",
    "fond-of-spryker/company-user-reference": "dev-master",
    "spryker/checkout-rest-api": "^1.1.0 || ^2.0.0 || ^3.0.0",
    "spryker/multi-cart": "^1.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "^0.12",
    "codeception/codeception": "^2.4",
    "phpro/grumphp": "^0.14",
    "sebastian/phpcpd": "^4.0",
    "php-coveralls/php-coveralls": "^2.1",
    "fond-of-codeception/spryker": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "sort-packages": true
    }
  }
}
